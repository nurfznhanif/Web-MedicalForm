{
  "name": "pt-bigs/medical-form-system",
  "description": "Medical Form System",
  "keywords": [
    "yii2",
    "framework",
    "medical",
    "form",
    "healthcare",
    "nursing"
  ],
  "homepage": "https://github.com/pt-bigs/medical-form-system",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/pt-bigs/medical-form-system/issues",
    "source": "https://github.com/pt-bigs/medical-form-system"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.2.0",
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap4": "^2.0",
    "yiisoft/yii2-swiftmailer": "~2.1.0",
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "yiisoft/yii2-jui": "~2.0.0",
    "yiisoft/yii2-bootstrap5": "^2.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/codeception": "^4.1.29 || ^5.0.0",
    "codeception/lib-innerbrowser": "^1.1.0 || ^3.0 || ^4.0",
    "codeception/module-asserts": "^1.3.0 || ^2.0.0 || ^3.0.0",
    "codeception/module-yii2": "^1.1.0",
    "codeception/module-filesystem": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "codeception/verify": "^2.2.0",
    "symfony/browser-kit": ">=2.7 <=6.2"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
